{
  "name": "excellentexport",
  "version": "3.0.0",
  "description": "Client side JavaScript export to Excel or CSV",
  "license": "MIT",
  "homepage": "http://jordiburgos.com/post/2014/excellentexport-javascript-export-to-excel-csv.html",
  "author": "Jordi Burgos <jordiburgos@gmail.com>",
  "bugs": "https://github.com/jmaister/excellentexport/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jmaister/excellentexport.git"
  },
  "keywords": [
    "excel",
    "export",
    "csv",
    "javascript",
    "client",
    "side"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js --progress --watch",
    "prod": "webpack --config webpack.config.js --progress -p",
    "test": "mocha-webpack --watch --webpack-config webpack.config-tests.js \"test/**/*.test.js\" ",
    "test-ci": "mocha-webpack --webpack-config webpack.config-tests.js \"test/**/*.test.js\"",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test-ci",
    "devtest": "webpack-dev-server --config webpack.config-tests.js",
    "mh": "mocha-webpack --help"
  },
  "nyc": {
    "include": [
      "excellentexport.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "main": "dist/excellentexport.js",
  "dependencies": {
    "xlsx": "^0.11.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "blob-polyfill": "^1.0.20150320",
    "cross-env": "^5.0.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "mocha": "^3.5.3",
    "mocha-webpack": "^0.7.0",
    "nyc": "^11.2.1",
    "webdriverio": "^4.8.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-node-externals": "^1.6.0"
  }
}
